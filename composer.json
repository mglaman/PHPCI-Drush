{
  "name": "mglaman/phpci-drush",
  "description": "Adds Drush to PHPCI",
  "minimum-stability": "stable",
  "type": "library",
  "license": "MIT",
  "require": {
    "drush/drush": "7.0.0-rc2"
  },
  "autoload": {
    "psr-4": { "mglaman\\PhpciPlugins\\": "src" }
  },
  "extra": {
    "phpci": {
      "pluginNamespace": "mglaman\\PhpciPlugins\\",
      "suppliedPlugins": [
        {"name": "Drush", "class": "DrushPlugin"}
      ]
    }
  }
}
